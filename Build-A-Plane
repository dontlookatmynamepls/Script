local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local Window = Library:NewWindow("Flandre Hub")

local Section = Window:NewSection("Main")

Section:CreateButton("teleport", function()
local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Dịch chuyển CFrame theo trục X cộng thêm 
humanoidRootPart.CFrame = humanoidRootPart.CFrame + Vector3.new(99999999999999999, 0, 0)

wait(1)

game.Players.LocalPlayer.Character:BreakJoints()

print("Clicked")
end)

Section:CreateButton("Launch and click the button☝️", function()
print("Clicked")
end)

Section:CreateButton("khởi động và bấm nút trên☝️", function()
print("Clicked")
end)

Section:CreateButton("script làm bởi tiktok @flandrescript", function()
print("Clicked")
end)

Section:CreateButton("script make by tiktok @flandrescript", function()
print("Clicked")
end)

